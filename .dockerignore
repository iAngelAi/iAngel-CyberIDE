node_modules/
npm-debug.log
yarn-error.log
.npm/

# IDE and Editor
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Git
.git/
.gitignore

# Docker (don't include in context)
Dockerfile*
docker-compose*.yml
.dockerignore

